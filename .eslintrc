{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "globals": {
    "Apps": true,
    "Turfmapp": true,
    "React": true,
    "ReactDOM": true,
    "Reflux": true,
    "ReactRouter": true,
    "gettext": true,
    "classNames": true,
    "Utils": true,
    "$": true,
    "_": true,
    "moment": true
  },
  "rules": {
    "camelcase": [
      0
    ],
    "comma-dangle": [
      0
    ],
    "comma-spacing": [
      1
    ],
    "curly": [
      1
    ],
    "eqeqeq": [
      1
    ],
    "indent": [
      1,
      4
    ],
    "new-cap": [
      0
    ],
    "no-alert": [
      0
    ],
    "no-dupe-keys": [
      2
    ],
    "no-extra-semi": [
      1
    ],
    "no-multi-spaces": [
      1
    ],
    "no-undef": [
      1
    ],
    "no-underscore-dangle": [
      0
    ],
    "no-unreachable": [
      1
    ],
    "no-unused-vars": [
      1,
      {
        "vars": "all",
        "args": "after-used"
      }
    ],
    "no-use-before-define": [
      0
    ],
    "quotes": [
      0,
      "single"
    ],
    "react/jsx-no-undef": 2,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 2,
    "semi": [
      1
    ],
    "strict": [
      0,
      "function"
    ]
  }
}